{
    "name": "feycot/page-analyzer",
    "autoload": {
        "files": [
            "src/Schema.php",
            "src/App.php"
        ],
        "psr-4": {
            "Feycot\\PageAnalyzer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Feycot\\PageAnalyzer\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "feycot",
            "email": "feycot@gmail.com"
        }
    ],
    "require": {
        "slim/slim": "^4.10",
        "slim/psr7": "^1.5",
        "slim/http": "^1.2",
        "slim/php-view": "^3.2",
        "php-di/php-di": "^6.4",
        "illuminate/database": "^9.21",
        "vlucas/phpdotenv": "^5.4",
        "guzzlehttp/guzzle": "^7.0",
        "imangazaliev/didom": "^2.0",
        "slim/flash": "^0.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "*",
        "symfony/var-dumper": "*",
        "nekofar/slim-test": "^2.0",
        "php-vcr/php-vcr": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
